{
  "name": "ps",
  "version": "0.1.0",
  "author": "Alexander Gabriel",
  "type": "module",
  "scripts": {
    "backend:start": "npx electric-sql start --with-postgres",
    "backend:stop": "npx electric-sql stop",
    "backend:up": "npx electric-sql start --with-postgres --detach",
    "backend:down": "npx electric-sql stop --remove",
    "client:generate:debug": "npx electric-sql generate --debug",
    "client:generate": "npx electric-sql generate && node renameLables.js",
    "client:watch": "npx electric-sql generate --watch",
    "db:migrate": "npx electric-sql with-config \"npx pg-migrations apply --database {{ELECTRIC_PROXY}} --directory ./db/migrations\"",
    "db:psql": "npx electric-sql psql",
    "electric:start": "npx electric-sql start",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@css-hooks/react": "2.0.2",
    "@css-hooks/recommended": "2.0.2",
    "@cyntler/react-doc-viewer": "1.14.1",
    "@fluentui/react-components": "9.46.8",
    "@fluentui/react-datepicker-compat": "0.4.26",
    "@fluentui/react-icons": "2.0.232",
    "@fluentui/react-timepicker-compat-preview": "0.2.0",
    "@fontsource/roboto": "5.0.12",
    "@formkit/tempo": "0.0.15",
    "@kripod/uuidv7": "0.3.4",
    "@react-leaflet/core": "2.1.0",
    "@shopify/react-web-worker": "5.0.13",
    "@turf/bbox": "6.5.0",
    "@turf/buffer": "6.5.0",
    "@turf/helpers": "6.5.0",
    "@uploadcare/blocks": "0.35.2",
    "allotment": "1.20.0",
    "dayjs": "1.11.10",
    "dotenv": "16.4.5",
    "electric-sql": "0.9.4",
    "framer-motion": "11.0.8",
    "is-uuid": "1.0.2",
    "leaflet": "1.9.4",
    "leaflet-draw": "1.0.4",
    "lodash": "4.17.21",
    "proj4": "2.10.0",
    "proj4leaflet": "1.0.2",
    "react": "18.2.0",
    "react-colorful": "5.6.1",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.13",
    "react-icons": "5.0.1",
    "react-imask": "7.5.0",
    "react-leaflet": "4.2.1",
    "react-linkify": "1.0.0-alpha",
    "react-resize-detector": "10.0.1",
    "react-router-dom": "6.22.3",
    "react-xml-viewer": "2.0.1",
    "recharts": "2.12.2",
    "redaxios": "0.5.1",
    "use-debounce": "10.0.0",
    "wa-sqlite": "rhashimoto/wa-sqlite#semver:v0.9.12",
    "wms-capabilities": "0.6.0"
  },
  "devDependencies": {
    "@databases/pg-migrations": "5.0.3",
    "@types/leaflet": "1.9.8",
    "@types/leaflet-draw": "1.0.11",
    "@types/proj4": "2.5.5",
    "@types/proj4leaflet": "1.0.10",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "typescript": "5.4.2",
    "vite": "5.1.5"
  },
  "devRemarks": {
    "electric-sql": "do not upgrade from 0.9.2 to 0.9.3, see: https://github.com/electric-sql/electric/issues/977"
  }
}
