{
  "name": "ps",
  "version": "0.1.0",
  "main": "builder.js",
  "scripts": {
    "backend:start": "node ./backend/startCompose.js",
    "backend:stop": "docker compose --env-file backend/compose/.envrc -f backend/compose/docker-compose.yaml stop",
    "backend:up": "npm run backend:start --detach",
    "backend:down": "docker compose --env-file backend/compose/.envrc -f backend/compose/docker-compose.yaml down --volumes",
    "client:generate": "npm run electric:check && npx electric-sql generate --service http://localhost:5133 --proxy postgresql://prisma:proxy_password@localhost:65432/electric && node renameLables.js",
    "client:watch": "npm run client:generate --watch",
    "db:migrate": "node ./db/migrate.js",
    "db:psql": "node ./db/connect.js",
    "ports:configure": "node ./change-ports.js",
    "electric:start": "node ./backend/startElectric.js",
    "electric:check": "node ./check-electric-is-running.js",
    "build": "node copy-wasm-files.js && node builder.js",
    "build-prod": "cross-env NODE_ENV=production npm run build",
    "dev": "npm run electric:check && cross-env SERVE=true npm run build"
  },
  "engines": {
    "node": ">=16.11.0"
  },
  "dependencies": {
    "@fluentui/react-components": "9.44.3",
    "@fluentui/react-datepicker-compat": "0.4.15",
    "@fluentui/react-icons": "2.0.225",
    "@fluentui/react-timepicker-compat-preview": "0.2.0",
    "@fontsource/roboto": "5.0.8",
    "@kripod/uuidv7": "0.3.4",
    "@preact/signals": "1.2.2",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "antd": "5.12.8",
    "dayjs": "1.11.10",
    "electric-sql": "0.8.2",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1",
    "react-imask": "7.3.0",
    "react-router-dom": "6.21.2",
    "wa-sqlite": "rhashimoto/wa-sqlite#semver:v0.9.9"
  },
  "devDependencies": {
    "@databases/pg-migrations": "5.0.3",
    "@types/node": ">=20.11.0",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "cross-env": "7.0.3",
    "esbuild": "0.16.17",
    "esbuild-plugin-inline-image": "0.0.9",
    "fs-extra": "11.2.0",
    "prompt": "1.3.0",
    "shelljs": "0.8.5",
    "tcp-port-used": "1.0.2",
    "typescript": "5.3.3"
  },
  "comments": {
    "esbuild": "do not update, breaks"
  }
}
